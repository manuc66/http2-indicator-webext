{
  "manifest_version": 2,
  "name": "HTTP/2 Indictor",
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "catgifs@mozilla.org"
    }
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "HTTP/2 indicator",
    "default_icon": {
      "38": "icon38.png"
    }
  }
}